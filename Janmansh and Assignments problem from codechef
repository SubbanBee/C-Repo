#include <stdio.h>

int main() {
	// your code goes here
     int T;
     scanf("%d",&T);
     while(T--)
     {
         int X;
         scanf("%d",&X);
         if(10-X>=3)
        {
            printf("YES\n");
        }
        else
        {
            printf("NO\n");
        }
     }
     return 0;
}

