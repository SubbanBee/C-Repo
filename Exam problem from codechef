#include <stdio.h>

int main() {
    // your code goes here
    int T;
    scanf("%d", & T);
    while (T--)
    {
        int X, Y, Z;
        scanf("%d%d%d", & X, & Y, & Z);
        int M = (Z * 100 / X * Y);
        if (2*Z>X*Y)
        {
            printf("YES\n");
        }
        else
        {
            printf("NO\n");
        }
    }
    return 0;
}
